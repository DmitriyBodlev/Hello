{
  "version": 1,
  "public": true,
  "name": "my-nextjs-app",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "builds": [
    { "src": "*.html", "use": "@now/static" },
    { "src": "*.js", "use": "@now/node" },
  ],
  "routes": [
    { "src": "/custom-page", "dest": "/index.html" },
    { "src": "/.*", "dest": "https://my-old-site.com"},
  ]
}
